	<script>
	var interleft;
	var interright
	$(document).ready(function(e) {

		$('#rightbox').mouseover(function(){
				if(interright)
					clearTimeout(interleft);
				$('#rightart').animate({right:"0px"},500);		
		});

		$('#rightbox').mouseout(function(){
				interright = setTimeout(function(){
					$('#rightart').animate({right:"-460px"},500);		
				},1000);
		});

		$('#rightart').mouseover(function(){
				clearTimeout(interright);
		});

		$('#rightart').mouseout(function(){
				clearTimeout(interright);
				interright = setTimeout(function(){
					$('#rightart').animate({right:"-460px"},500);		
				},1000);
		});

		$('#leftbox').mouseover(function(){
				if(interleft)
					clearTimeout(interleft);
				$('#leftart').animate({left:"0px"},500);		
		});

		$('#leftbox').mouseout(function(){
				interleft = setTimeout(function(){
					$('#leftart').animate({left:"-460px"},500);		
				},1000);
		});

		$('#leftart').mouseover(function(){
				clearTimeout(interleft);
		});

		$('#leftart').mouseout(function(){
				clearTimeout(interleft);
				interleft = setTimeout(function(){
					$('#leftart').animate({left:"-460px"},500);		
				},1000);
		});

		$(document).keydown(function(e){
			getkey(e.which);
		});

		if($('.news_content_related').length>0){
			var iframeparen= $('#dummyhtml').find('p');
			var str =  $('.news_content_related').html();
			$('.news_content_related').remove();
			var strhtml  = '<div class="news_content_related">'+str+'</div>';
			$(iframeparen[0]).after(strhtml);
		}
	});

	function getkey(key){ 
		if(key == 39 || key == 75){
			location.href = encodeURIComponent("-telugu-news-10538");
		}else if(key == 37 || key == 74){
			location.href = encodeURIComponent("after-chiranjeevi-pawan-two-superstars-for-a-film-telugu-news-178315"); 

		}
	}
