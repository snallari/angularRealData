	<script>
	$(document).ready(function(e) {
	    getSlideInfo();
	});

	var parenturl = "/rajendra-prasads-lip-service-ends-in-trouble-telugu-news-10547";
	function setStates(e){
		var din = e['data']['index'];
		din = Number(din) - 1;	
		if(currentslideindex != din){
			currentslideindex = din;	
			loadSlide();
		}
		try{
		ga('send', 'pageview', e['hash']);}catch(e){
		}
	}

	var objslide;
	var currentslideindex = -1;
	function getSlideInfo(){
		objslide = [];
		var getTitles = $('.slidemain .slide .title');
		var getimages = $('.slidemain .slide img');
		var getSlides = $('.slidemain .slide');
		$(getSlides).each(function(index, element) {
			objslide[index] = new Object();
			objslide[index].title = $(this).find('.title').text();
			objslide[index].img = $(this).find('img').attr('src');
			$(this).find('.title').remove();
			$(this).find('img').remove();
			objslide[index].slidehtml = $(this).html();
			
		});
		$('.slidemain').css('display','none');
		if(objslide.length>0){
			loadSlideNext();
		}else{
			$('.post-container').css('display','none');
		}
		
	}
	function loadSlideNext(){
		if(currentslideindex < (objslide.length -1)){
			currentslideindex++;
			loadSlide()
		}
	}

	function loadSlide(){

		if(currentslideindex == 0){
			$('.slideprevbt').addClass('slidedisable');	
		}else{
			$('.slideprevbt').removeClass('slidedisable');		
		}

		if(currentslideindex == objslide.length-1){
			$('.slidenextbt').addClass('slidedisable');
		}else{
			$('.slidenextbt').removeClass('slidedisable');
		}

		if(objslide.length>0){
		var obj = objslide[currentslideindex];
		$('.post-container').fadeOut('fast',function(){
		$('.post-thumb img').attr('src',obj.img);
		$('.post-title').html(obj.title);
		$('.post-content').html(obj.slidehtml); 
		$('.post-container').fadeIn('fast');
		});

		var historyobj = new Object();
		historyobj.index = currentslideindex+1;
			var url = parenturl +"-slide"+(currentslideindex+1)
			console.log(url);
		try{
			History.pushState(historyobj, "puri-to-be-the-president-of-baalayyas-fan-association", url);
		}catch(e){}
			var countermessage = (currentslideindex+1)+" of "+objslide.length;
			$('.post-counter').html(countermessage);
			reloadAds();
		}
	}

	function loadSlidePrev(){
		if(currentslideindex >0){
			currentslideindex--;
			loadSlide();
		}
	}

	function setSlideIndex(x){
			currentslideindex = x-1;
			loadSlide();
	}

	function reloadAds(){
		var d = $('.adsarticle');
		$(d).each(function(i,item){ 
		$(this).attr('src', $(this).attr('src')); 
		});
	}
	</script>

	<script>
	function closeadfloating(){
		var now = new Date();
		var time = now.getTime();
		time += 6*3600 * 1000;
		now.setTime(time);
		document.cookie = "floatad=closed; expires="+ now.toUTCString() +"; path=/";
		document.getElementById('floatads300x250').innerHTML = "";
	}

	function getCookieFloatad(cname) {
	    var name = cname + "=";
	    var ca = document.cookie.split(';');
	    for(var i=0; i<ca.length; i++) {
	        var c = ca[i];
	        while (c.charAt(0)==' ') c = c.substring(1);
	        if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
	    }
	    return "";
	}

	function handlerforad(){
		var pos = $('.fb-comments').position().top;
		var sche = screen.availHeight;
		var doctop = $(document).scrollTop();
		var totalhe = sche + doctop;
		if(pos<totalhe && document.getElementById('floatads300x250').innerHTML != ""){
			$('#floatads300x250').slideUp();
		}else{
			$('#floatads300x250').slideDown();
		}
	}
	$(document).ready(function(e) {
	    $(window).on('DOMContentLoaded load resize scroll', handlerforad);
	});
	</script>
</body>